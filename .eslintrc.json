{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/order": "off",
        "no-else-return": "off",
        "linebreak-style": "off",
        "newline-per-chained-call": "off"
    }
}
